<comps>
  <category>      
    <id>Base-system</id>
    <name>base-system mode installation</name>
    <displayOrder>1</displayOrder>
    <description>Select this installation type to install a text environment without X system in Advantech devices.</description>
    <localPackageList>
      <localPackage>qt-fb</localPackage>
    </localPackageList>
  </category>

  <category>
    <id>Desktop-system</id>
    <name>Desktop-system mode installation</name>
    <displayOrder>2</displayOrder>
    <description>This option installs a Desktop environment with xorg X server and system anministration in Advantech devices.</description>
    <localPackageList>
      <localPackage>qt-x11</localPackage>
      <localPackage>qt-creator</localPackage>
    </localPackageList>
  </category>

  <group>
    <id>TPC-8100TR</id>
    <description>Drivers installed on TPC-8100TR.</description>
    <packageList>
      <packageReq>adspname</packageReq>
      <packageReq>advec</packageReq>
      <packageReq>penmount</packageReq>
    </packageList>
    <settingList>
      <grubReq>intel_idle.max_cstate=1;net.ifnames=0</grubReq>
      <keyboardReq>true</keyboardReq>
    </settingList>
  </group>
  
